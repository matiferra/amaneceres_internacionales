{
    "name": "kamecode/auth",
    "description": "Auth for Laravel",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name":     "KameCode",
            "email":    "info@kame-code.com",
            "homepage": "https://kame-code.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "6.0|^7.0|^8.0",
        "illuminate/filesystem": "6.0|^7.0|^8.0",
        "illuminate/support": "6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "KameCode\\Auth\\": "src/",
            "Illuminate\\Foundation\\Auth\\": "illuminate-auth/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KameCode\\Auth\\KameAuthServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
